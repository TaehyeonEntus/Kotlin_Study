<?xml version="1.0" encoding="utf-8"?>

<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent" xmlns:app="http://schemas.android.com/apk/res-auto"
        android:orientation="vertical"
        tools:context=".MainActivity">
    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="136dp">
        <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:cardCornerRadius="50dp"
                android:layout_margin="10dp">
            <ImageView
                    android:id="@+id/member1"
                    android:src="@drawable/maple2"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    android:layout_weight="1"/>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:cardCornerRadius="50dp"
                android:layout_margin="10dp">
            <ImageView
                    android:id="@+id/member2"
                    android:src="@drawable/maple3"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    android:layout_weight="1"/>
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:cardCornerRadius="50dp"
                android:layout_margin="10dp">
            <ImageView
                    android:id="@+id/member3"
                    android:src="@drawable/maple4"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    android:layout_weight="1"/>
        </androidx.cardview.widget.CardView>
    </LinearLayout>

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="130dp">
        <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:cardCornerRadius="50dp"
                android:layout_margin="10dp">
            <ImageView
                    android:id="@+id/member4"
                    android:src="@drawable/maple5"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    android:layout_weight="1"/>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:cardCornerRadius="50dp"
                android:layout_margin="10dp">
            <ImageView
                    android:id="@+id/member5"
                    android:src="@drawable/maple6"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    android:layout_weight="1"/>
